<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nino.hrckaraoke"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.nino.hrckaraoke.Login"
            android:label="@string/app_name" >
        </activity>
        <activity android:name="com.nino.hrckaraoke.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.nino.hrckaraoke.Request"
            android:label="@string/title_activity_request" >
        </activity>
        <activity
            android:name="com.nino.hrckaraoke.Register"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name="com.nino.hrckaraoke.Profile"
            android:label="@string/title_activity_profile" >
        </activity>
        <activity
            android:name="com.nino.hrckaraoke.Previous"
            android:label="@string/title_activity_previous" >
        </activity>
        <activity
            android:name="com.nino.hrckaraoke.Others"
            android:label="@string/title_activity_others" >
        </activity>
        <activity
            android:name="com.nino.hrckaraoke.Guide"
            android:label="@string/title_activity_guide" >
        </activity>
        <activity
            android:name="com.nino.hrckaraoke.Welcome"
            android:label="@string/title_activity_welcome"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.nino.hrckaraoke.Search"
            android:label="@string/title_activity_search" >
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>